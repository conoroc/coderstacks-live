<div class="well">
  <% @resources.each do |resource| %>
      <%= link_to(resource_path(resource)) do %>
          <%= resource.title %>


          <% if resource.image? %>
              <%= image_tag(resource.image_url, :class => "img-thumbnail") %>
          <% else %>
              <%= image_tag('CS.gif', :class => "img-thumbnail") %>
          <% end %>
      <% end %>
  <% end %>
</div>